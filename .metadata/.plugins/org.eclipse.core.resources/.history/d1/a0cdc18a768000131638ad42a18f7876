package com.jonathan.math;

public class Cell 
{
	/** Stores the row and column of the cell. */
	private int row, col;
	
	public Cell()
	{
	}
	
	public Cell(int row, int col)
	{
		this.row = row;
		this.col = col;
	}
	
	public void moveLeft()
	{
		row--;
	}
	
	public int getRow()
	{
		return row;
	}
	
	public void setRow(int row)
	{
		this.row = row;
	}
	
	public int getCol()
	{
		return col;
	}
	
	public void setCol(int col)
	{
		this.col = col;
	}
}
